package dominio;

public class Persona {
	
	private String nombre; 
	private int dni;
	
	// contructors
	
	Public Persona()
	{
		String nombre = "vacio";
		int dni = 0; 
	}
	
	Public Persona(String nombre, int dni)
	{
		
	}
	// setters and getters
	public String getNombre() {
		return nombre;
	}
	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
	public int getDni() {
		return dni;
	}
	public void setDni(int dni) {
		this.dni = dni;
	}

}
